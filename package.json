{
  "name": "app-hackton",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "teste:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/app/use-cases",
    "test:watch": "vitest --dir src/app/use-cases",
    "pretest:e2e": "run-s teste:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/app/http",
    "test:e2e:watch": "vitest --dir src/app/http",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "build": "tsup src --out-dir build"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/morgan": "1.9.9",
    "@types/multer": "1.4.11",
    "@types/node": "22.1.0",
    "@types/supertest": "6.0.2",
    "@types/swagger-ui-express": "4.1.6",
    "@vitest/coverage-v8": "2.0.4",
    "@vitest/ui": "2.0.4",
    "dotenv": "16.4.5",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "npm-run-all": "4.1.5",
    "prisma": "5.18.0",
    "supertest": "7.0.0",
    "swagger-ui-express": "5.0.1",
    "tsup": "8.2.4",
    "tsx": "4.16.5",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4",
    "vitest-tsconfig-paths": "3.4.1"
  }
}
